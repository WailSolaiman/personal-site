        "serve": "gatsby serve",
        "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
    },
    "dependencies": {
        "devicon": "^2.2.0",
        "gatsby": "^2.18.17",
        "gatsby-image": "^2.2.37",
